  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>select 0 Row_Num,'অচল/ক্রটিপূর্ণ প্রকৌশল যন্ত্রপাতির বিবরন' ReportName,SerialNo,DM.ReportDate as ReportDate ,Mi.Machinery Description,cc.BTypeValue MobilityDescription,DamageDate,RepairTime,DM.Remarks,CI.ControlName ShipName,DATENAME(MONTH, ReportDate)+', ' + Cast(year(ReportDate) as Varchar(4)) ReportManth,'' ReportName1,'' MachineSerialNo1,'' ReportDate1,'' MachineName1,'' MachineSide1,'' Hour1,'' Minute1,'' Duration1,'' ShipName1,'' MachineRemarks1,'' ReportName2,'' FuelSerialNo2,'' FuelName2,'' ReportDate2,'' StockFuelOil2,'' FuelConsumption2,'' UnitPrice2,'' TotalPrice2,'' ShipName2,'' GrandTotal2,0 ETotal2,'' LastOil,'' ReceiveOil,'' DonateOil from DamageMachineriesInfo DM
inner join ControlShipInfo CI on DM.ShipId =CI.ControlShipInfoId
left join MachineryInfo Mi on DM.Description = Mi.MachineryInfoIdentity
left join CommonCode cc on DM.MobilityDescription = cc.CommonCodeID
where DM.MobilityDescription not in(974,983) and  DM.Description is not null and DM.ShipId =(Case when ''='' then DM.ShipId else '' end) and DM.ReportDate &lt;= DATEADD(s,-1,DATEADD(mm, DATEDIFF(m,0,'')+1,0))
union all
select 0 Row_Num,'','','','','','','','','','', 'প্রকৌশল বিভাগের  যন্ত্রপাতির পরিসংখ্যান নিম্নে প্রদত্ত হল' ReportName,MachineSerialNo,DM.ReportDate,Mi.Machinery MachineName,MachineSide,Hour,Minute,Duration,CI.ControlName  ShipName,DM.MachineRemarks,'','','','','','','','','','',0,'','','' from DamageMachineriesInfo DM
inner join ControlShipInfo CI on DM.ShipId = CI.ControlShipInfoId
left join MachineryInfo Mi on DM.MachineName = Mi.MachineryInfoIdentity
where DM.MachineName is not null and DM.ShipId =(Case when ''='' then DM.ShipId else '' end) and DM.ReportDate between DATEADD(MONTH, DATEDIFF(MONTH, 0, ''), 0) and  DATEADD(s,-1,DATEADD(mm, DATEDIFF(m,0,'')+1,0))
union all
select Row_Number() over(Order by DM.FuelOilId) Row_Num,'','','','','','','','','','','','','','','','','','','','', 'পি ও এল এর হিসাব' ReportName,FuelSerialNo,cc.BTypeValue  AS FuelName,DM.ReportDate as ReportDate,dbo.FnGetBanglaAmount(StockFuelOil) StockFuelOil,dbo.FnGetBanglaAmount(FuelConsumption) FuelConsumption,dbo.FnGetBanglaAmount(UnitPrice) UnitPrice,
dbo.FnGetBanglaAmount(Cast(FuelConsumption as decimal) * Cast(UnitPrice as decimal)) as
TotalPrice,CI.ControlName ShipName,(Select dbo.FnGetBanglaAmount(Sum(Cast(FuelConsumption as decimal) * Cast(UnitPrice as decimal))) from DamageMachineriesInfo where  FuelOilId is not null and ShipId =(Case when ''='' then DM.ShipId else '' end) and DM.LastUpdate  &lt;= '') as GrandTotal
,Cast(FuelConsumption as decimal) * Cast(UnitPrice as decimal) as ETotal 
,dbo.FnGetBanglaAmount(((Cast(StockFuelOil as decimal) + Cast(ReceivedOil as decimal)) - (Cast(FuelConsumption as decimal) + Cast(DonateOil as decimal)))) as LastOil,dbo.FnGetBanglaAmount(ReceivedOil) ReceivedOil,dbo.FnGetBanglaAmount(DonateOil) DonateOil

from DamageMachineriesInfo DM
inner join ControlShipInfo CI on DM.ShipId = CI.ControlShipInfoId
inner Join CommonCode cc on cc.CommonCodeID=DM.FuelOilId
where DM.FuelOilId is not null and DM.ShipId =(Case when ''='' then DM.ShipId else '' end) and DM.ReportDate between DATEADD(MONTH, DATEDIFF(MONTH, 0, ''), 0) and DATEADD(s,-1,DATEADD(mm, DATEDIFF(m,0,'')+1,0))</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
