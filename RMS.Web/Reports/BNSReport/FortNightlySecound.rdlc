      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>select re.LastRefitFrom,re.LastRefitTo,'' DateFrom,'' DateTo,re.LastDockingFrom,re.LastDockingTo,d.* from (Select  WorkCategoy,ControlShipIdentity,c.TypeValue WorkName,dbo.GetControlShipName(ControlShipIdentity) ShipName,cc.TypeValue DrType
,(SELECT STUFF((SELECT ',' + CAST(No AS VARCHAR(50)) DrNo FROM FortNightly WHERE WorkCategoy = t.WorkCategoy  and No in(Select No from FortNightly where WorkCategoy=t.WorkCategoy and ControlShipIdentity =t.ControlShipIdentity and DrType =t.DrType and TwentyFive=1 and Fifty=0 and SeventyFive=0 and Complete=0 )  FOR XML PATH(''), TYPE).value('.','NVARCHAR(MAX)'),1,1,'') DrNo FROM FortNightly t where t.WorkCategoy=m.WorkCategoy and t.ControlShipIdentity =m.ControlShipIdentity and t.DrType =m.DrType group by t.WorkCategoy,t.ControlShipIdentity,t.DrType) TFPercent 
,(SELECT STUFF((SELECT ',' + CAST(No AS VARCHAR(50)) DrNo FROM FortNightly WHERE WorkCategoy = t.WorkCategoy and No in(Select No from FortNightly where WorkCategoy=t.WorkCategoy and ControlShipIdentity =t.ControlShipIdentity and DrType =t.DrType  and Fifty=1 and SeventyFive=0 and Complete=0 )  FOR XML PATH(''), TYPE).value('.','NVARCHAR(MAX)'),1,1,'') DrNo FROM FortNightly t where t.WorkCategoy=m.WorkCategoy and t.ControlShipIdentity =m.ControlShipIdentity and t.DrType =m.DrType group by t.WorkCategoy,t.ControlShipIdentity,t.DrType) FPercent 
,(SELECT STUFF((SELECT ',' + CAST(No AS VARCHAR(50)) DrNo FROM FortNightly WHERE WorkCategoy = t.WorkCategoy and No in(Select No from FortNightly where WorkCategoy=t.WorkCategoy and ControlShipIdentity =t.ControlShipIdentity and DrType =t.DrType and SeventyFive=1 and Complete=0)  FOR XML PATH(''), TYPE).value('.','NVARCHAR(MAX)'),1,1,'') DrNo FROM FortNightly t where t.WorkCategoy=m.WorkCategoy and t.ControlShipIdentity =m.ControlShipIdentity and t.DrType =m.DrType group by t.WorkCategoy,t.ControlShipIdentity,t.DrType) SFPercent 
,(SELECT STUFF((SELECT ',' + CAST(No AS VARCHAR(50)) DrNo FROM FortNightly WHERE WorkCategoy = t.WorkCategoy and No in(Select No from FortNightly where WorkCategoy=t.WorkCategoy and ControlShipIdentity =t.ControlShipIdentity and DrType =t.DrType and Complete=1) FOR XML PATH(''), TYPE).value('.','NVARCHAR(MAX)'),1,1,'') DrNo FROM FortNightly t where t.WorkCategoy=m.WorkCategoy and t.ControlShipIdentity =m.ControlShipIdentity and t.DrType =m.DrType  group by t.WorkCategoy,t.ControlShipIdentity,t.DrType) HundredPercent
,(SELECT count(*) FROM FortNightly t where t.WorkCategoy=m.WorkCategoy and t.ControlShipIdentity =m.ControlShipIdentity and t.DrType = m.DrType group by t.WorkCategoy,t.ControlShipIdentity,t.DrType) TotalCount 
,(SELECT count(*) FROM FortNightly t where t.WorkCategoy=m.WorkCategoy and t.ControlShipIdentity =m.ControlShipIdentity and t.DrType =m.DrType and No in(Select No from FortNightly where WorkCategoy=m.WorkCategoy and ControlShipIdentity =m.ControlShipIdentity and TwentyFive=0 and Fifty=0 and SeventyFive=0 and Complete=0) group by t.WorkCategoy,t.ControlShipIdentity,t.DrType) NotWorkCount 
,(SELECT STUFF((SELECT ',' + CAST(No AS VARCHAR(50)) DrNo FROM FortNightly WHERE WorkCategoy = t.WorkCategoy  and No in(Select No from FortNightly where WorkCategoy=t.WorkCategoy and ControlShipIdentity =t.ControlShipIdentity and DrType =t.DrType and TwentyFive=0 and Fifty=0 and SeventyFive=0 and Complete=0 )  FOR XML PATH(''), TYPE).value('.','NVARCHAR(MAX)'),1,1,'') DrNo FROM FortNightly t where t.WorkCategoy=m.WorkCategoy and t.ControlShipIdentity =m.ControlShipIdentity and t.DrType =m.DrType group by t.WorkCategoy,t.ControlShipIdentity,t.DrType) NotWorking 
from FortNightly m
left join CommonCode c on m.WorkCategoy = c.CommonCodeID
left join CommonCode cc on m.DrType = cc.CommonCodeID
where m.CreateDate between '' and  ''
 group by c.TypeValue,WorkCategoy,ControlShipIdentity,cc.TypeValue,DrType) d
right join RefitDockingSchedule re on d.ControlShipIdentity = re.ControlShipInfoId
 where re.LastRefitFrom ='' and re.LastRefitTo = '' and re.ControlShipInfoId =''</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
