        </Field>
        <Field Name="StockAfterUseLtrOrUnit">
          <DataField>StockAfterUseLtrOrUnit</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="OilPerLtRate">
          <DataField>OilPerLtRate</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="TotalExpense">
          <DataField>TotalExpense</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DataSource2</DataSourceName>
        <CommandText>select miford.Machinery as MachineName, dm.DefectDate, dm.DefectReasonDetail,'' as Machinery,0 as RunningHr, 0 as RunningMin ,
'' as TypeValue, 0 as InitialStockLtrOrUnit, 0 as ReceivedQtyLtrorUnit, 0 as HandoverLtrOrUnit, 0 as MonthlyUseltrOrUnit, 0 as StockAfterUseLtrOrUnit, 0 OilPerLtRate, 0 as TotalExpense 

from MonthlyReturn mr
left join ControlShipInfo csi on mr.ShipId=csi.ControlShipInfoId
left join DefectMachinary dm on mr.MonthlyReturnId=dm.MonthlyReturnId
left join MachineryInfo miford on dm.MachineId=miford.MachineryInfoIdentity
where mr.MonthlyReturnId=40062

union all

----------------------
select '' as MachineName, '' as DefectDate, '' as DefectReasonDetail,miformrh.Machinery,mrh.RunningHr, mrh.RunningMin  ,
'' as TypeValue, 0 as InitialStockLtrOrUnit, 0 as ReceivedQtyLtrorUnit, 0 as HandoverLtrOrUnit, 0 as MonthlyUseltrOrUnit, 0 as StockAfterUseLtrOrUnit, 0 OilPerLtRate, 0 as TotalExpense 

from MonthlyReturn mr
left join MachinaryRunningHour mrh on mrh.MonthlyReturnId=mr.MonthlyReturnId
left join MachineryInfo miformrh on mrh.NoOPSMachineId=miformrh.MachineryInfoIdentity
where mr.MonthlyReturnId=40062
---------------------------------
union all
select '' as MachineName, '' as DefectDate, '' as DefectReasonDetail,'' as Machinery,0 as RunningHr, 0 as RunningMin ,
cm.TypeValue, polei.InitialStockLtrOrUnit, polei.ReceivedQtyLtrorUnit, polei.HandoverLtrOrUnit, polei.MonthlyUseltrOrUnit, polei.StockAfterUseLtrOrUnit, polei.OilPerLtRate, polei.MonthlyUseltrOrUnit*polei.OilPerLtRate as TotalExpense 

from MonthlyReturn mr
left join POLExpenseInfo polei on polei.MonthlyReturnId=mr.MonthlyReturnId
left join CommonCode cm on cm.CommonCodeID=polei.OilName
where mr.MonthlyReturnId=40062</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
